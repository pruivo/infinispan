<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0">
   <localRepository>${user.home}/.m2/rhdg-8-repository</localRepository>

   <profiles>
      <profile>
         <id>indy-temporary-build</id>
         <repositories>
            <repository>
               <id>indy-temp-repository</id>
               <name>Indy Temporary Builds Maven Repository</name>
               <url>https://indy.psi.redhat.com/api/content/maven/hosted/temporary-builds/</url>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </snapshots>
            </repository>
         </repositories>
         <pluginRepositories>
            <pluginRepository>
               <id>indy-temp-plugins-repository</id>
               <name>Indy Temporary Builds Maven Repository</name>
               <url>https://indy.psi.redhat.com/api/content/maven/hosted/temporary-builds/</url>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </snapshots>
            </pluginRepository>
         </pluginRepositories>
      </profile>

      <profile>
         <id>indy-everything</id>
         <repositories>
            <repository>
               <id>indy-static-repository</id>
               <name>Indy Maven Repository</name>
               <url>https://indy.psi.redhat.com/api/content/maven/group/static</url>
               <releases>
                  <enabled>true</enabled>
               </releases>
               <snapshots>
                  <enabled>false</enabled>
               </snapshots>
            </repository>
         </repositories>
         <pluginRepositories>
            <pluginRepository>
               <id>indy-static-plugin-repository</id>
               <name>Indy Maven Plugin Repository</name>
               <url>https://indy.psi.redhat.com/api/content/maven/group/static</url>
               <releases>
                  <enabled>true</enabled>
               </releases>
               <snapshots>
                  <enabled>false</enabled>
               </snapshots>
            </pluginRepository>
         </pluginRepositories>
      </profile>

      <profile>
         <id>redhat-ea-repository</id>
         <repositories>
            <repository>
               <id>redhat</id>
               <name>Red Hat Early Access Maven Repository</name>
               <url>https://maven.repository.redhat.com/earlyaccess/all/</url>
               <layout>default</layout>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </snapshots>
            </repository>
         </repositories>
         <pluginRepositories>
            <pluginRepository>
               <id>redhat</id>
               <name>Red Hat Early Access Maven Repository</name>
               <url>https://maven.repository.redhat.com/earlyaccess/all/</url>
               <layout>default</layout>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </snapshots>
            </pluginRepository>
         </pluginRepositories>
      </profile>

      <profile>
         <id>redhat-ga-repository</id>
         <repositories>
            <repository>
               <id>redhat</id>
               <name>Red Hat Maven Repository</name>
               <url>https://maven.repository.redhat.com/ga/</url>
               <layout>default</layout>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </snapshots>
            </repository>
         </repositories>
         <pluginRepositories>
            <pluginRepository>
               <id>redhat</id>
               <name>Red Hat Maven Repository</name>
               <url>https://maven.repository.redhat.com/ga/</url>
               <layout>default</layout>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>never</updatePolicy>
               </snapshots>
            </pluginRepository>
         </pluginRepositories>
      </profile>

   </profiles>

   <activeProfiles>
      <activeProfile>indy-temporary-build</activeProfile>
      <activeProfile>indy-everything</activeProfile>
      <activeProfile>redhat-ga-repository</activeProfile>
      <activeProfile>redhat-ea-repository</activeProfile>
   </activeProfiles>

</settings>
